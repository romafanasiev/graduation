{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "react-app",
      "airbnb",
      "airbnb-typescript",
      "plugin:import/typescript",
      "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "overrides": [
    ],
    "parserOptions": {
      "ecmaFeatures": {
            "jsx": true
        },
      "ecmaVersion": "latest",
      "sourceType": "module",
      "project": "./tsconfig.json"
    },
    "plugins": [
      "react",
      "@typescript-eslint",
      "prettier"
    ],
    "rules": {
    }
}
